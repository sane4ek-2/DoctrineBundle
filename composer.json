{
    "name": "doctrine/doctrine-bundle",
    "type": "symfony-bundle",
    "description": "Symfony DoctrineBundle",
    "keywords": ["DBAL", "ORM", "Database", "Persistence"],
    "homepage": "http://www.doctrine-project.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        },
        {
            "name": "Benjamin Eberlei",
            "email": "kontakt@beberlei.de"
        },
        {
            "name": "Symfony Community",
            "homepage": "http://symfony.com/contributors"
        },
        {
            "name": "Doctrine Project",
            "homepage": "http://www.doctrine-project.org/"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1",
        "symfony/framework-bundle": "~2.7|~3.0|~4.0",
        "symfony/console": "~2.7|~3.0|~4.0",
        "symfony/dependency-injection": "~2.7|~3.0|~4.0",
        "jdorn/sql-formatter": "~1.1",
        "symfony/doctrine-bridge": "~2.7|~3.0|~4.0",
        "doctrine/doctrine-cache-bundle": "~1.2",
        "doctrine/dbal": "dev-2.7-sharding"
    },
    "require-dev": {
        "symfony/yaml": "~2.7|~3.0|~4.0",
        "symfony/validator": "~2.7|~3.0|~4.0",
        "symfony/property-info": "~2.8|~3.0|~4.0",
        "symfony/phpunit-bridge": "~2.7|~3.0|~4.0",
        "twig/twig": "~1.12|~2.0",
        "satooshi/php-coveralls": "^1.0",
        "phpunit/phpunit": "^6.1",
        "doctrine/orm": "dev-2.6-sharding"
    },
    "suggest": {
        "symfony/web-profiler-bundle": "To use the data collector.",
        "doctrine/orm": "The Doctrine ORM integration is optional in the bundle."
    },
    "autoload": {
        "psr-4": { "Doctrine\\Bundle\\DoctrineBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.7.x-dev"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/sane4ek-2/doctrine2"
        },
        {
            "type": "git",
            "url": "https://github.com/sane4ek-2/dbal"
        }
    ]
}
